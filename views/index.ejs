	<link rel="icon" href="images/logo/favicon.ico" type="image/x-icon">
	<link rel="stylesheet" href="libraries/bootstrap/bootstrap.min.css">
	<link rel="stylesheet" href="libraries/animate.css">
	<link rel="stylesheet" href="styles/css/index.css">
</head>

<body>
	<% include partials/header.ejs %>

	<main role="main" class="bg-light">
		<div class="container">
			<section id="about" class="wow animated fadeInUp py-5">
				<h2 class="text-center mt-md-5">About us</h2>
				<hr>

				<div class="row mt-5">
					<div class="col-md">
						<img src="images/illustrations/webD.svg" class="w-100 h-100" />
					</div>

					<div class="col-md py-5">
						<p class="lead">
							We’re a software development company that is dedicated to providing world-class innovative,
							scalable, technology solutions.
						</p>
					</div>
				</div>

				<div class="row mt-4">
					<div class="col-md order-md-2">
						<img src="images/illustrations/teach.svg" class="w-100 h-100" />
					</div>

					<div class="col-md py-5 order-md-1">
						<p class="lead pt-lg-4">
							We’re a software development company that is dedicated to providing world-class innovative,
							scalable, technology solutions.
						</p>
					</div>
				</div>
			</section>

			<section id="services" class="wow animated fadeInUp py-5">
				<h2 class="text-center mt-md-5">What we do</h2>
				<hr>

				<div class="mt-4">
					<p class="lead text-center">We offer a variety of awesome services for you to choose from, or by all
						means, choose them all.
					</p>

					<div class="row mt-5 justify-content-center">
						<div class="col-md-4 col-6">
							<div class="card h-100 pt-3 pb-2 px-sm-2  rounded border-0 shadow-sm">
								<div class="card-body text-center">
									<span class="bgPrimary p-3 rounded-circle d-inline-block">
										<img src="images/icons/personalC.svg" class="img-fluid" style="width:35px" />
									</span>

									<span class="cardHeader mt-4">Web design and development</span>
									<p class="mt-2">We provide creative and dynamic websites and webapps.</p>
								</div>
							</div>
						</div>

						<div class="col-md-4 col-6">
							<div class="card h-100 pt-3 pb-2 px-sm-2  rounded border-0 shadow-sm">
								<div class="card-body text-center">
									<span class="bgPrimary p-3 rounded-circle d-inline-block">
										<img src="images/icons/uix.svg" class="img-fluid" style="width:35px" />
									</span>

									<span class="cardHeader mt-4">UI/UX design</span>
									<p class="mt-2">We find out what captivates users and design based on what we find
										out</p>
								</div>
							</div>
						</div>
					</div>

					<div class="row mt-5 justify-content-center">
						<div class="col-md-4 col-6">
							<div class="card h-100 pt-3 pb-2 px-sm-2  rounded border-0 shadow-sm">
								<div class="card-body text-center">
									<span class="bgPrimary p-3 rounded-circle d-inline-block">
										<img src="images/icons/mobile.svg" class="img-fluid" style="width:35px" />
									</span>

									<span class="cardHeader mt-4">Mobile app development</span>
									<p class="mt-2">Mobile development is much more than just an app.</p>
								</div>
							</div>
						</div>

						<div class="col-md-4 col-6">
							<div class="card h-100 pt-3 pb-2 px-sm-2  rounded border-0 shadow-sm">
								<div class="card-body text-center">
									<span class="bgPrimary p-3 rounded-circle d-inline-block">
										<img src="images/icons/graphicDesign.svg" class="img-fluid"
											style="width:35px" />
									</span>

									<span class="cardHeader mt-4">Graphics and logo design</span>
									<p class="mt-2">We design logos, fliers, banners, posters for your businesses</p>
								</div>
							</div>
						</div>
					</div>
				</div>
			</section>

			<section id="portfolio" class="wow animated fadeInUp py-5">
				<h2 class="text-center mt-md-5">Our portfolio</h2>
				<hr>

				<div class="mt-4">
					<div class="row mt-5 justify-content-center">
						<div class="col-6 col-lg-4">
							<div class="card h-100 pt-3 pb-2 px-sm-2  rounded shadow-sm">
								<div class="card-body">
									<span class="cardHeader">Tolumi The Fashion People</span>
									<p class="mt-2">UI-UX design • Web development</p>
									<button class="btn btn-sm bgPrimary text-white mb-5 py-0 px-2"><span
											class="small">View project</span></button>
									<img src="images/shabak.jpg" class="w-100 img-fluid" />
								</div>
							</div>
						</div>

						<div class="col-6 col-lg-4">
							<div class="card h-100 pt-3 pb-2 px-sm-2  rounded shadow-sm">
								<div class="card-body">
									<span class="cardHeader">Experience Technologies</span>

									<p class="mt-2">Graphics and logo design</p>

									<button class="btn btn-sm bgPrimary text-white mb-5 py-0 px-2">
										<span class="small">View project</span>
									</button>

									<img src="images/shabak.jpg" class="w-100 img-fluid" />
								</div>
							</div>
						</div>

						<div class="col-6 col-lg-4 mt-5 mt-lg-0 mr-auto">
							<div class="card h-100 pt-3 pb-2 px-sm-2  rounded shadow-sm">
								<div class="card-body">
									<span class="cardHeader">Folashewa photography</span>
									<p class="mt-2">UI-UX design</p>
									<button class="btn btn-sm bgPrimary text-white mb-5 py-0 px-2"><span
											class="small">View project</span></button>
									<img src="images/shabak.jpg" class="w-100 img-fluid" />
								</div>
							</div>
						</div>
					</div>

					<div class="text-center mt-5">
						<a href="/">
							See all projects
							<img 
								src="images/icons/down-chevron.svg"
								class="img-fluid i1 h-100 my-auto animated heartBeat infinite slower" style="width:11px"
							/>
						</a>
					</div>
				</div>
		</div>
		</section>

		<section id="contact" class="wow animated fadeInUp py-5">
			<h2 class="text-center mt-md-5">Talk to us</h2>
			<hr>

			<div class="mt-4">
				<p class="text-center lead">Send us a ring and we’ll get back to you as soon as we can. Feel
					free to use the contact form below or send us an email <a href="#">here</a></p>

				<div class="col-md-8 col-lg-6 m-auto">
					<div class="form-group mt-5">
						<label class="lead" for="name">Name/Company Name</label>
						<input type="text" class="form-control shadow-sm bg-light py-4" placeholder="john doe" />
					</div>

					<div class="form-group">
						<label class="lead" for="email">Email</label>
						<input type="email" class="form-control shadow-sm bg-light py-4"
							placeholder="johndoe@gmail.com" />
					</div>

					<div class="form-group my-4">
						<label class="lead" for="phoneNumber">Phone Number</label>
						<input type="tel" class="form-control shadow-sm bg-light py-4"
							placeholder="(+1) 234 ___ ___ ___" />
					</div>

					<div class="form-group">
						<label class="lead" for="name">Message</label>
						<textarea class="form-control shadow-sm bg-light py-4" rows="3"
							placeholder="Register for a course, Tell us about your project or just say hello"></textarea>
					</div>

					<button type="submit" class="btn btn-block bgPrimary text-white mt-4 py-3">Send Message</button>
				</div>
			</div>
		</section>

		<div class="scrollTopButton">
			<span class="d-flex mb-4">
				<a data-scroll href="#" class="ml-auto">
					<img src="images/icons/up-chev.svg" class="h-100 img-fluid" style="width: 2.8em" />
				</a>
			</span>
		</div>
		</div>
